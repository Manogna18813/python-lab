#without recursion:-
print("fibonacci with iteration!")
n=int(input("Enter a number: "))
def fib(n):
    a,b=0,1
    for _ in range(n):
        print(a,end=" ")
        a,b=b,a+b
    print()
fib(n)

#with recursion:-
print("fibonacci with recurssion!")
def fib(n):
        if n==0:
            return 0
        elif n==1:
            return 1
        else:
            return fib(n-1)+fib(n-2)
for i in range(n):
     print(fib(i),end=" ")
print()
